{
  "identifier": "robb-j.Gitfox",
  "name": "Gitfox",
  "organization": "robb-j",
  "description": "Open a Nova workspace's repository in Gitfox.app",
  "version": "1.0.0",
  "categories": ["commands"],
  "license": "MIT",
  "repository": "https://github.com/robb-j/nova-gitfox",
  "bugs": "https://github.com/robb-j/nova-gitfox/issues",
  "main": "main.dist.js",
  "activationEvents": [],
  "entitlements": {
    "process": true,
    "filesystem": "readonly"
  },
  "commands": {
    "editor": [],
    "extensions": [
      {
        "command": "robb-j.gitfox.open-repository",
        "title": "Open it Gitfox"
      }
    ]
  },
  "config": [
    {
      "key": "robb-j.gitfox.gfPath",
      "title": "Custom path",
      "type": "string",
      "description": "Set this to provide a custom path for the `gitfox` binary. If not set, a `which` process is used find the binary."
    }
  ],
  "configWorkspace": []
}
