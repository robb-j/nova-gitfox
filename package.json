{
  "name": "nova-gitfox",
  "private": true,
  "scripts": {
    "format": "prettier --write '**/*.{js,ts,tsx,json,css,md}'"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/nova-editor-node": "^4.1.3",
    "esbuild": "^0.14.1",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.0",
    "typescript": "^4.5.2",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,css,md}": [
      "prettier --write"
    ]
  }
}
